<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xmlns="http://www.springframework.org/schema/beans" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://dubbo.apache.org/schema/dubbo
       http://dubbo.apache.org/schema/dubbo/dubbo.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">


<!-- base config -->
    <dubbo:application name="foxnut">
        <dubbo:parameter key="qos.enable" value="true"/>
        <dubbo:parameter key="qos.accept.foreign.ip" value="false"/>
        <dubbo:parameter key="qos.port" value="55555"/>
    </dubbo:application>
    <dubbo:registry address="zookeeper://192.168.15.26:2181?backup=192.168.15.27:2181,192.168.15.28:2181" timeout="5000"/>
<!--    <dubbo:metadata-report address="zookeeper://192.168.15.26:2181?backup=192.168.15.27:2181,192.168.15.28:2181"/>-->
    <dubbo:reference id="demoProviderService" check="false" interface="com.zzrc.sdk.lotus.service.UserIdentityRpcService"/>
<!--    <dubbo:reference interface="org.zzrc.common.BaseCase" id="xxxService" check="false" timeout="30000"/>-->

</beans>